{
  "name": "reacttmpl",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.1",
    "bulma-react": "0.0.8",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-sass-chokidar": "0.0.3",
    "prop-types": "^15.5.8",
    "react": "^16.0.0",
    "react-addons-css-transition-group": "^15.6.0",
    "react-cookie": "^2.1.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.3",
    "react-render-html": "^0.5.0",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-sortable-hoc": "^0.6.3",
    "react-sparklines": "^1.6.0",
    "redux": "^3.6.0",
    "redux-form": "^7.0.4",
    "redux-promise-middleware": "^4.2.1",
    "redux-saga": "^0.15.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-info": "^3.2.9",
    "@storybook/addon-knobs": "^3.2.8",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/addon-notes": "^3.2.7",
    "@storybook/react": "^3.2.8",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "chai": "^4.1.2",
    "enzyme": "^2.9.1",
    "faker": "^4.1.0",
    "jasmine-core": "^2.6.2",
    "npm-run-all": "^4.1.1",
    "react-addons-test-utils": "^15.6.0",
    "react-scripts": "1.0.14",
    "react-test-renderer": "^16.0.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-extension": "^2.13.2",
    "sinon": "^2.4.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "postbuild": "rm -rvf build/*/*/*.map",
    "clean": "rm -rvf build coverage jest* src/*/*/*.css  src/*/*.css  src/*.css",
    "test": "react-scripts test --env=jsdom",
    "testc": "react-scripts test --env=jsdom --coverage",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  }
}
